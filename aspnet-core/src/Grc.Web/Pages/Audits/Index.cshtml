@page
@using Microsoft.Extensions.Localization
@using Grc.Localization
@model Grc.Web.Pages.Audits.IndexModel
@inject IStringLocalizer<GrcResource> L

<abp-card>
    <abp-card-header>
        <h2><i class="fas fa-clipboard-check text-info"></i> @L["Menu:AuditManagement"]</h2>
    </abp-card-header>
    <abp-card-body>
        <abp-table striped-rows="true" id="AuditsTable">
            <thead>
                <tr>
                    <th>@L["AuditCode"]</th>
                    <th>@L["Name"]</th>
                    <th>@L["Type"]</th>
                    <th>@L["StartDate"]</th>
                    <th>@L["Status"]</th>
                    <th>@L["Actions"]</th>
                </tr>
            </thead>
        </abp-table>
    </abp-card-body>
</abp-card>

