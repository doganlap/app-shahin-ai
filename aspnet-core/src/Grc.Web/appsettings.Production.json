{
  "App": {
    "SelfUrl": "http://37.27.139.173:5500",
    "CorsOrigins": "http://37.27.139.173:5500,http://localhost:5500,https://grc.shahin-ai.com",
    "BlobStoragePath": "/var/lib/grc/blobstorage"
  },
  "AuthServer": {
    "Authority": "http://37.27.139.173:5500",
    "ClientId": "Grc_Web",
    "ClientSecret": "1q2w3e*",
    "RequireHttpsMetadata": false
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5434;Database=GRCDatabase;Username=postgres;Password=postgres"
  },
  "AbpBlobStoring": {
    "S3": {
      "Endpoint": "${S3_ENDPOINT}",
      "BucketName": "${S3_BUCKET_NAME}",
      "AccessKeyId": "${S3_ACCESS_KEY_ID}",
      "SecretAccessKey": "${S3_SECRET_ACCESS_KEY}",
      "UseSSL": true
    }
  },
  "Redis": {
    "Configuration": "localhost:6379"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "/var/log/grc/web-.log",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 10485760,
          "retainedFileCountLimit": 30,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}



